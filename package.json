{
  "name": "lab50",
  "displayName": "CS50 Lab",
  "description": "https://cs50.readthedocs.io/lab/",
  "icon": "images/lab50.png",
  "version": "1.0.17",
  "publisher": "CS50",
  "repository": "https://github.com/cs50/lab50.vsix",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Education"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lab50.openAsLab",
        "title": "Open in CS50 Lab",
        "icon": "$(book)",
        "enablement": "lab50:didActivateExtension"
      },
      {
        "command": "lab50.resetLayout",
        "title": "Reset Layout",
        "icon": "$(layout-panel-right)"
      },
      {
        "command": "lab50.closeLab",
        "title": "Close Lab",
        "icon": "$(panel-close)"
      }
    ],
    "customEditors": [
      {
        "viewType": "lab50.editor",
        "displayName": "CS50 Lab",
        "selector": [
          {
            "filenamePattern": "*.md"
          }
        ],
        "priority": "option"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "lab50.openAsLab",
          "when": "false"
        }
      ],
      "explorer/context": [
        {
          "command": "lab50.openAsLab",
          "when": "explorerResourceIsFolder || resourceExtname == '.md' ",
          "group": "2_workspace@0"
        }
      ],
      "view/title": [
        {
          "command": "lab50.resetLayout",
          "group": "navigation@2",
          "when": "view == lab50"
        },
        {
          "command": "lab50.closeLab",
          "group": "navigation@3",
          "when": "view == lab50"
        }
      ]
    },
    "views": {
      "lab50": [
        {
          "id": "lab50",
          "name": "CS50 Lab",
          "type": "webview",
          "when": "lab50:showReadme"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lab50",
          "title": "CS50 Lab",
          "icon": "$(book)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/markdown-it": "^12.2.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "vsce": "^2.11.0"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.5",
    "axios": "^0.27.2",
    "highlight.js": "^11.6.0",
    "html-entities": "^2.3.3",
    "js-yaml": "^4.1.0",
    "liquidjs": "^9.39.1",
    "luxon": "^3.0.3",
    "markdown-it": "^13.0.1",
    "markdown-it-attrs": "^4.1.4",
    "markdown-it-highlightjs": "^4.0.1",
    "node-html-parser": "^6.0.0",
    "pretty": "^2.0.0"
  }
}
